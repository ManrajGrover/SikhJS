{
  "name": "sikhjs",
  "version": "0.0.1",
  "description": "Multiplatform application for Gurbani",
  "main": "index.js",
  "dependencies": {
    "bootstrap": "^3.3.5",
    "fs-promise": "^0.3.1",
    "jquery": "^2.1.4",
    "markdown-it": "^4.4.0"
  },
  "devDependencies": {},
  "scripts": {
    "start": "electron .",
    "package-darwin": "rm -rf SikhJS* && electron-packager . SikhJS --platform=darwin --arch=all --version=0.34.0",
    "package-win32": "rm -rf SikhJS* && electron-packager . SikhJS --platform=win32 --arch=ia32 --version=0.34.0",
    "package-linux": "rm -rf SikhJS* && electron-packager . SikhJS --platform=linux --arch=ia32 --version=0.34.0",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
